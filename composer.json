{
    "name": "storipress/webflow",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Storipress\\Webflow\\": "src/"
        }
    },
    "require": {
        "php": "~8.1",
        "illuminate/http": "^10",
        "illuminate/support": "^10"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "~7.8",
        "laravel/pint": "~1.13",
        "nunomaduro/larastan": "~2.6",
        "ergebnis/composer-normalize": "~2.36.0",
        "pestphp/pest-plugin-laravel": "~2.2.0",
        "orchestra/testbench": "^8.10"
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true
        }
    }
}
